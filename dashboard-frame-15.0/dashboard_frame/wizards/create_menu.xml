<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_model_menu_create" model="ir.ui.view">
        <field name="name">Create Menu</field>
        <field name="model">smart.analytics.dashboard.create.menu</field>
        <field name="arch" type="xml">
            <form string="Create Menu">
                <group>
                    <field name="parent_menu_id" required="1"/>
                    <field name="name" required="1"/>
                </group>
                <footer>
                    <button name="menu_create" string="Create Menu" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
