#. module: dashboard_frame
#: model:mail.template,subject:dashboard_frame.dashboard_screenshot_email_template
msgid "${object.subject}"
msgstr ""

#. module: dashboard_frame
#: model:mail.template,body_html:dashboard_frame.dashboard_screenshot_email_template
msgid ""
"<div style=\"margin: 0px; padding: 0px; font-family: Arial, Helvetica, sans-serif;\">\n"
"                    <h2 style=\"color: #1f497d;\">Dashboard Screenshot: ${object.dashboard_name}</h2>\n"
"                    <p>Dear Recipient,</p>\n"
"                    <p>${object.body | safe}</p>\n"
"                    <p style=\"margin-top: 20px;\">\n"
"                        You can find the dashboard screenshot attached to this email.\n"
"                    </p>\n"
"                    <div style=\"margin-top: 20px;\">\n"
"                        <p>Best regards,</p>\n"
"                        <p>${user.name}</p>\n"
"                    </div>\n"
"                    <div style=\"margin-top: 20px; font-size: 12px; color: #888888;\">\n"
"                        <p>This email was sent from ${user.company_id.name}.</p>\n"
"                        <p>If you have any questions, please contact us at ${user.company_id.email}.</p>\n"
"                    </div>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px; font-family: Arial, Helvetica, sans-serif;\">\n"
"                    <h2 style=\"color: #1f497d;\">Capture d'écran du tableau de bord : ${object.dashboard_name}</h2>\n"
"                    <p>Cher destinataire,</p>\n"
"                    <p>${object.body | safe}</p>\n"
"                    <p style=\"margin-top: 20px;\">\n"
"                        La capture d'écran du tableau de bord est jointe à cet e-mail.\n"
"                    </p>\n"
"                    <div style=\"margin-top: 20px;\">\n"
"                        <p>Je vous prie d'agréer, Madame, Monsieur, l'expression de mes salutations distinguées,</p>\n"
"                        <p>${user.name}</p>\n"
"                    </div>\n"
"                    <div style=\"margin-top: 20px; font-size: 12px; color: #888888;\">\n"
"                        <p>Ce courriel a été envoyé par ${user.company_id.name}.</p>\n"
"                        <p>Si vous avez des questions, veuillez nous contacter à l'adresse suivante ${user.company_id.email}.</p>\n"
"                    </div>\n"
"                </div>\n"
"            "

